<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="3"
  SkinType="Component"
  Comment="HTML5 Search Bar skin"
  ComponentType="SearchBar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="Search Bar">
        <Properties>
          <Property
            Name="BorderTopLeftRadius">0px</Property>
          <Property
            Name="BorderTopRightRadius">0px</Property>
          <Property
            Name="BorderBottomRightRadius">0px</Property>
          <Property
            Name="BorderBottomLeftRadius">0px</Property>
          <Property
            Name="Height">2em</Property>
          <Property
            Name="Width">100%</Property>
          <Property
            Name="MarginTop">0px</Property>
          <Property
            Name="BorderTop">solid 1px #d0d3d5</Property>
          <Property
            Name="BorderRight">solid 1px #d0d3d5</Property>
          <Property
            Name="BorderBottom">solid 1px #d0d3d5</Property>
          <Property
            Name="BorderLeft">solid 1px #d0d3d5</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Submit Button">
        <Properties>
          <Property
            Name="BackgroundColor">transparent</Property>
          <Property
            Name="Width">33px</Property>
          <Property
            Name="BackgroundImage">url('icon-searchGray-17x17.png')</Property>
        </Properties>
      </Style>
      <Medium
        Name="Tablet">
        <Style
          Name="Search Bar">
          <Properties>
            <Property
              Name="Display">none</Property>
          </Properties>
        </Style>
      </Medium>
      <Style
        Name="Search Input">
        <Properties>
          <Property
            Name="Color">#45494c</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Filter Drop-down">
        <Properties>
          <Property
            Name="Color">#45494c</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Auto-complete">
        <Properties>
          <Property
            Name="BorderTop">solid 1px #45494c</Property>
          <Property
            Name="BorderRight">solid 1px #45494c</Property>
          <Property
            Name="BorderBottom">solid 1px #45494c</Property>
          <Property
            Name="BorderLeft">solid 1px #45494c</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Auto-complete Item (selected)">
        <Properties>
          <Property
            Name="BackgroundColor">#f3f4f4</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Auto-complete Item: Description">
        <Properties>
          <Property
            Name="Color">#45494c</Property>
        </Properties>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="Search-gray.png"></Resource>
      <Resource
        Name="icon-searchGray-17x17.png"></Resource>
    </Resources>
    <Stylesheet></Stylesheet>
  </Stylesheet>
  <Strings />
</CatapultSkin>